    "vue": "^2.6.11",
    "vue-analytics": "^5.22.1",
    "vue-class-component": "^7.2.3",
    "vue-meta": "^2.3.2",
    "vue-property-decorator": "^8.4.2",
    "vuera": "^0.2.7",
    "website-components": "^1.0.0"
  },
  "devDependencies": {
    "@gridsome/source-filesystem": "^0.6.2",
    "@gridsome/transformer-json": "^0.2.1",
    "@gridsome/transformer-remark": "^0.5.0",
    "@tyankatsu0105/types-gridsome": "^0.6.0",
    "@types/d3": "^5.7.2",
